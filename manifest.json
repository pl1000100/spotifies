{
    "manifest_version": 3,
    "name": "Spotifies",
    "version": "0.1",
    "description": "Chrome Extension Spotify Manager.",
    "permissions": [
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "assets/icons/icon16.png",
      "48": "assets/icons/icon48.png",
      "128": "assets/icons/icon128.png"
    }
  }
  